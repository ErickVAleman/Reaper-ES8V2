{
  "name": "reaperes8",
  "version": "2.0.503",
  "description": "Reaper-ES8 boilerplate basado en express",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build": "yarn clean && babel src --out-dir build --source-maps inline",
    "start": "node build/www/index.js",
    "debug": "nodemon --inspect ./build/www/index.js",
    "dev": "nodemon src/www/index.js --exec babel-node"
  },
  "author": "Erick Villalobos Aleman",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "morgan": "^1.9.0",
    "sequelize": "^4.38.0",
    "tedious": "^2.6.4",
    "vhost": "^3.0.2"
  }
}
